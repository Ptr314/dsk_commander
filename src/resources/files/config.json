{
    "file_formats": {
        "FILE_RAW_MSB": {
            "order":        10,
            "name":         "RAW image",
            "short_name":   "DSK",
            "source":       true,
            "extensions":   "*.dsk;*.do;*.po;*.cpm;*.gmd",
            "types":        ["TYPE_AGAT_140", "TYPE_AGAT_840", "TYPE_PC_360_I", "TYPE_PC_360_NI", "TYPE_GMD_7012_I"]
        },
        "FILE_AIM": {
            "order":        20,
            "name":         "Agat AIM",
            "short_name":   "AIM",
            "source":       true,
            "extensions":   "*.aim",
            "types":        ["TYPE_AGAT_840"]
        },
        "FILE_HXC_MFM": {
            "order":        30,
            "name":         "HxC MFM file",
            "short_name":   "MFM",
            "source":       true,
            "extensions":   "*.mfm",
            "types":        ["TYPE_AGAT_140"]
        },
        "FILE_HXC_HFE": {
            "order":        30,
            "name":         "HxC HFE file",
            "short_name":   "HFE",
            "source":       true,
            "extensions":   "*.hfe",
            "types":        ["TYPE_AGAT_840"]
        },
        "FILE_MFM_NIB": {
            "order":        30,
            "name":         "NIB MFM file",
            "short_name":   "NIB",
            "source":       true,
            "extensions":   "*.nib",
            "types":        ["TYPE_AGAT_140", "TYPE_AGAT_840"]
        },
        "FILE_MFM_NIC": {
            "order":        30,
            "name":         "NIC MFM file",
            "short_name":   "NIC",
            "source":       true,
            "extensions":   "*.nic",
            "types":        ["TYPE_AGAT_140"]
        },
        "FILE_IMD": {
            "order":        30,
            "name":         "IMD image",
            "short_name":   "IMD",
            "source":       true,
            "extensions":   "*.imd",
            "types":        ["TYPE_PC_360_I", "TYPE_PC_360_NI"]
        },
        "FILE_BINARY": {
            "order":        30,
            "name":         "Binary File",
            "short_name":   "BIN",
            "source":       false,
            "extensions":   "*.*",
            "types":        []
        },
        "FILE_FIL": {
            "order":        20,
            "name":         "Agat FIL",
            "short_name":   "FIL",
            "source":       true,
            "extensions":   "*.fil",
            "types":        ["TYPE_FIL"]
        },
        "FILE_SOS": {
            "order":        30,
            "name":         "Agat Sprite OS File",
            "short_name":   "SOS",
            "source":       false,
            "extensions":   "*.sos",
            "types":        []
        },
        "FILE_ANY": {
            "order":        0,
            "name":         "All Files",
            "short_name":   "ALL",
            "source":       true,
            "extensions":   "*",
            "types":        []
        },
        "FILE_SUPPORTED": {
            "order":        5,
            "name":         "All Supported",
            "short_name":   "Supported",
            "source":       true,
            "extensions":   "*.*",
            "types":        []
        }
    },
    "file_types": {
        "TYPE_AGAT_840": {
            "name": "Agat 840K",
            "targets": ["FILE_RAW_MSB", "FILE_HXC_HFE"],
            "filesystems": ["FILESYSTEM_DOS33", "FILESYSTEM_SPRITE_OS"]
        },
        "TYPE_AGAT_140": {
            "name": "Agat/Apple 140K",
            "targets": ["FILE_RAW_MSB", "FILE_HXC_MFM", "FILE_MFM_NIB", "FILE_MFM_NIC"],
            "filesystems": ["FILESYSTEM_DOS33", "FILESYSTEM_SPRITE_OS", "FILESYSTEM_CPM_DOS", "FILESYSTEM_CPM_PRODOS", "FILESYSTEM_CPM_RAW"]
        },
        "TYPE_FIL": {
            "name": "Agat .FIL container",
            "targets": [],
            "filesystems": ["FILESYSTEM_FIL"]
        },
        "TYPE_PC_360_I": {
            "name": "360K Interleaved",
            "targets": ["FILE_RAW_MSB"],
            "filesystems": ["FILESYSTEM_CPM_RAW"]
        },
        "TYPE_PC_360_NI": {
            "name": "360K Sequential",
            "targets": ["FILE_RAW_MSB"],
            "filesystems": ["FILESYSTEM_CPM_RAW"]
        },
        "TYPE_GMD_7012_I": {
            "name": "GMD-7012 8\" 256 Kb",
            "targets": ["FILE_RAW_MSB"],
            "filesystems": ["FILESYSTEM_CPM_RAW"]
        }
    },
    "file_systems": {
        "FILESYSTEM_DOS33": {
            "name": "Agat/Apple DOS33"
        },
        "FILESYSTEM_SPRITE_OS": {
            "name": "Agat Sprite OS"
        },
        "FILESYSTEM_FIL": {
            "name": "Single .FIL"
        },
        "FILESYSTEM_CPM_DOS": {
            "name": "CP/M (DOS 3.3 skew)"
        },
        "FILESYSTEM_CPM_PRODOS": {
            "name": "CP/M (ProDOS skew)"
        },
        "FILESYSTEM_CPM_RAW": {
            "name": "CP/M (No sector skew)"
        }
    }
